local KillingCheats = PhantomForcesWindow:NewSection("Hoop Dreams GUI")

KillingCheats:CreateButton("Inf Stamina (OP)", function()
   game:GetService("Players").LocalPlayer.PlayerGui.Values.Stamina.Regen:Destroy()
end)

KillingCheats:CreateButton("Anti Contest", function()
    local Number = 20
  workspace.Gravity = tonumber(Number) or 196.2 -- falls back to default gravity if its not a number or a parsable number
local mouse = game.Players.LocalPlayer:GetMouse()
		local parted = false
	
	
		local part = Instance.new("Part",game.Lighting)
		part.Anchored = true
		part.Size = Vector3.new(700,1,700)
		part.Transparency = 0.9
		part.TopSurface = "Smooth"
	
	
	
	
		mouse.KeyDown:connect(function(key)
			if key == "e" then
				game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(game.Players.LocalPlayer.Character:GetPrimaryPartCFrame()*CFrame.new(0, 12,0))
			end
		end)
end)

